{
  "name": "@willsofts/will-control",
  "version": "1.0.0",
  "description": "Vue Application Controls",
  "main": "dist/will-control.umd.js",
  "types": "index.d.ts",
  "files": [
    "dist/*",
    "src/*",
    "*.js"
  ],
  "scripts": {
    "build": "vue-cli-service build --target lib --name will-control src/index.js",
    "build:lib": "vue-cli-service build --target lib --name will-control src/index.js"
  },
  "keywords": [
    "DataPaging",
    "DataTable",
    "InputDate",
    "InputMask",
    "InputMoney",
    "InputNumber",
    "InputTime",
    "PageHeader"
  ],
  "author": "tassan_oro@freewillsolutions.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/willsofts/will-control/issues"
  },
  "homepage": "https://github.com/willsofts/will-control#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/willsofts/will-control.git"
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "jquery": "^3.7.1",
    "jquery-ui": "^1.13.3",
    "jquery-ui-dist": "^1.13.3",
    "vue": "^3.4.30"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@types/jquery": "^3.5.30",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
